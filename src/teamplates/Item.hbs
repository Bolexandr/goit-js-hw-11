{{#each this}}
<div class="photo-card">
  <img src={{this.largeImageURL}} alt={{this.tags}} loading="lazy" class="photo-card_Img"/>
  <div class="info">
    <p class="info-item">
      <b>Likes</b><br>{{this.likes}}
    </p>
    <p class="info-item">
      <b>Views</b><br>{{this.views}}
    </p>
    <p class="info-item">
      <b>Comments</b><br>{{this.comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b><br>{{this.downloads}}
    </p>
  </div>
</div>
{{/each}}